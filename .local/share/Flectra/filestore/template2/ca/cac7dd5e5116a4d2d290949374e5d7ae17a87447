/* /marketing_automation/static/src/js/libs/jquery.flowchart.js defined in bundle 'web.assets_backend' */
$(function(){$.widget("flowchart.flowchart",{options:{canUserEditLinks:true,canUserMoveOperators:true,data:{},distanceFromArrow:3,defaultOperatorClass:'flowchart-default-operator',defaultLinkColor:'#3366ff',defaultSelectedLinkColor:'black',linkWidth:5,grid:1,multipleLinksOnOutput:false,multipleLinksOnInput:false,linkVerticalDecal:0,onOperatorSelect:function(operatorId){return true;},onOperatorUnselect:function(){return true;},onOperatorMouseOver:function(operatorId,$el){return true;},onOperatorMouseOut:function(operatorId,$el){return true;},onLinkSelect:function(linkId){return true;},onLinkUnselect:function(){return true;},onOperatorCreate:function(operatorId,operatorData,fullElement){return true;},onLinkCreate:function(linkId,linkData){return true;},onOperatorDelete:function(operatorId){return true;},onLinkDelete:function(linkId,forced){return true;},onOperatorMoved:function(operatorId,position){},onAfterChange:function(changeType){}},data:null,objs:null,maskNum:0,linkNum:0,operatorNum:0,lastOutputConnectorClicked:null,selectedOperatorId:null,selectedLinkId:null,positionRatio:1,globalId:null,_create:function(){if(typeof document.__flowchartNumber=='undefined'){document.__flowchartNumber=0;}else{document.__flowchartNumber++;}
