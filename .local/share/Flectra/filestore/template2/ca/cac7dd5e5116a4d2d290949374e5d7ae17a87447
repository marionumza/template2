if(options.waiting_type){if(title.search('minutes')!==-1){title=title.replace('minutes',value);}else if(title.search('hours')!==-1){title=title.replace('hours',value);}else if(title.search('day')!==-1){title=title.replace('day',value);}else if(title.search('week')!==-1){title=title.replace('week',value);}else if(title.search('month')!==-1){title=title.replace('month',value);}
