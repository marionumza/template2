[0].indexOf('.')<0;},get_fields_widget:function(widget){var self=this;var pairs=self.get_pairs(field_registry);var widgets=pairs.filter(function(arr){return self.get_filtered_data(arr);});self.field_widgets=widgets.map(function(array){return array[0];})._wrapped;for(var i=0;i<self.field_widgets.length;i++){widget.push({id:i,text:self.field_widgets[i]})}
