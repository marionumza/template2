flectra.define('web.GraphController',function(require){"use strict";var AbstractController=require('web.AbstractController');var core=require('web.core');var qweb=core.qweb;var GraphController=AbstractController.extend({className:'o_graph',init:function(parent,model,renderer,params){this._super.apply(this,arguments);this.measures=params.measures;},destroy:function(){if(this.$buttons){this.$buttons.find('button').off().tooltip('destroy');}
