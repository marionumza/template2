<?xml version="1.0" encoding="utf-8"?>
<flectra>
<record id="sh_smps_adv_wizard_form_view" model="ir.ui.view">
	<field name="name">sh.smps.adv.wizard.form.view</field>
	<field name="model">smps.adv.wizard</field>
	<field name="arch" type="xml">
		<form string="Select Products Advance">	
			<field name="product_ids" domain="[('sale_ok', '=', True)]" options="{'no_create': True}"/>
			
			<footer>
				<button name="sh_smps_adv_select_btn" string="Select" type="object" class="oe_highlight"/>
				<button string="Cancel" class="oe_link" special="cancel" />
			</footer>			
		</form>

	</field>
	
</record>
</flectra>